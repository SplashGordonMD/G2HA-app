<h1>Edit if the most current contractors' information in incomplete</h1>
  <%= form_tag "/contractors/#{@contractor.id}", method: :patch do %>
<div>
    <%= label_tag :name %>
    <%= text_field_tag :name, @contractor.name %>
  </div>
  <div>
    <%= label_tag :phone_number %>
    <%= text_field_tag :phone_number, @contractor.phone_number %>
  </div>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email, @contractor.email %>
  </div>
  <div>
    <%= label_tag :web_site %>
    <%= text_field_tag :web_site, @contractor.web_site %>
  </div>
  <%= submit_tag "Submit" %>
<% end %>
</div>
</div>
